{
  "presets": [
    ["@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "entry",
        "corejs": "2"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-object-rest-spread", {
      "loose": true,
      "useBuiltIns": true
    }]
  ]
}
