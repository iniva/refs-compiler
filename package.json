{
  "name": "refs-compiler",
  "version": "1.0.0",
  "description": "Compiler for YAML, JSON and INI files using path references",
  "main": "lib/index.js",
  "author": "Tony Di Benedetto <tonyblast@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniva/refs-compiler.git"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "json",
    "yaml",
    "ini",
    "ref",
    "refs",
    "reference",
    "references",
    "yaml-ref",
    "json-ref",
    "compile"
  ],
  "bin": {
    "refs-compiler": "bin/compiler.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "audit": "npm audit",
    "lint": "eslint lib/ bin/ test/",
    "test": "lab -r console -c -a code -L",
    "test:coverage": "lab -r html -o coverage.html -a code -L",
    "test:coveralls": "lab -r lcov | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "ini": "^1.3.5",
    "node-yaml": "^3.1.1"
  },
  "devDependencies": {
    "code": "^5.2.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "lab": "^18.0.0"
  }
}
