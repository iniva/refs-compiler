{
  "name": "refs-compiler",
  "version": "1.0.1",
  "description": "Compiler for YAML, JSON and INI files using path references",
  "main": "lib/index.js",
  "author": "Tony Di Benedetto <tonyblast@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniva/refs-compiler.git"
  },
  "license": "MIT",
  "keywords": [
    "json",
    "yaml",
    "ini",
    "ref",
    "refs",
    "reference",
    "references",
    "yaml-ref",
    "json-ref",
    "compile"
  ],
  "bin": {
    "refs-compiler": "bin/compiler.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "commit": "git-cz",
    "hooks:message": "node hooks/index.js --action=message",
    "lint": "eslint src/",
    "lint:bin": "eslint bin/",
    "lint:test": "eslint test/",
    "test": "lab -r console -c -a code -L",
    "test:coverage": "lab -r html -o coverage.html -a code -L",
    "test:coveralls": "lab -r lcov | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "ini": "^1.3.5",
    "node-yaml": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "code": "^5.2.4",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.3.1",
    "lab": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
