{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": "airbnb-base",
  "rules": {
    "brace-style": ["error", "stroustrup"],
    "comma-dangle": "off",
    "no-multi-assign": "off",
    "no-prototype-builtins": "off"
  }
}
